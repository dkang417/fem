               {photos.map((photo, index) => (\n                        <img\n                            onClick={this.handleIndexClick}    \n                            key={photo.value}\n                            data-index={index}\n                            src={photo.value}\n                            className={index === active ? 'active' : ''}\n                            alt=\"animal thumbnail\"\n                        />    \n                ))}\n                </div>    \n            </div>    \n        )\n    }\n}\nexport default Carosel;\n"},"lineCount":null}},"hash":"4884f3fa8be32be5776777bbaa4fe8ca","cacheData":{"env":{}}}